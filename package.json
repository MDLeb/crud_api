    "format": "prettier --write \"src//*.ts\"",
    "lint": "eslint \"src//*.ts\" --fix"
  },
  "keywords": [
    "node.js",
    "crud"
  ],
  "author": "mdleb",
  "license": "ISC",
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@types/jest": "^29.5.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "uuid": "^9.0.1",
    "cross-env": "^7.0.3"
